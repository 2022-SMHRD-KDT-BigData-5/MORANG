<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="com.smhrd.mapper.noteMapper">
     <!-- 게시글 가져오기 -->
     <select id="noteList" resultType = "com.smhrd.domain.note">
         select * from note
     </select>
     
     <insert id="noteInsert" parameterType="com.smhrd.domain.note">
         insert into note(title, content, id)
         values(#{title}, #{content}, #{id})
     </insert>
     
 </mapper>